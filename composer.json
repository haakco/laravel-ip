{
  "name": "haakco/laravel-ip",
  "description": "Function to check and manage ip's. Also adds tables for storing.",
  "license": "MIT",
  "keywords": [
    "Laravel",
    "Ip"
  ],
  "authors": [
    {
      "name": "Tim Haak",
      "email": "tim@haak.co",
      "homepage": "https://haak.co"
    }
  ],
  "homepage": "https://github.com/haakco/laravel-ip",
  "require": {
    "php": ">=8.0",
    "haakco/laravel-postgres-helper": "^1.0",
    "illuminate/support": "~8 || ~9 || ~10 || ~11 || ~12",
    "torann/geoip": ">=3.0"
  },
  "require-dev": {
    "brainmaestro/composer-git-hooks": ">=2.8",
    "ergebnis/composer-normalize": ">=2.30",
    "laravel/pint": "^1.10",
    "phpunit/phpunit": ">=10.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "HaakCo\\Ip\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HaakCo\\Ip\\Tests\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    }
  },
  "extra": {
    "hooks": {
      "post-merge": "composer install",
      "pre-commit": [
        "echo committing as $(git config user.name)",
        "./vendor/bin/pint",
        "composer fix-code-format"
      ],
      "pre-push": []
    },
    "laravel": {
      "aliases": {
        "Ip": "HaakCo\\Ip\\Facades\\Ip"
      },
      "providers": [
        "HaakCo\\Ip\\IpServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "cghooks add --ignore-lock"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "cghooks update",
      "composer normalize"
    ],
    "cghooks": "vendor/bin/cghooks",
    "fix-code-format": [
      "./vendor/bin/pint || true"
    ],
    "lint": [
      "./vendor/bin/pint"
    ]
  }
}
