{
  "name": "haakco/laravel-ip",
  "description": "Function to check and manage ip's. Also adds tables for storing.",
  "license": "proprietary",
  "keywords": [
    "Laravel",
    "Ip"
  ],
  "authors": [
    {
      "name": "Tim Haak",
      "email": "tim@haak.co",
      "homepage": "https://haak.co"
    }
  ],
  "homepage": "https://github.com/haakco/laravel-ip",
  "require": {
    "geoip2/geoip2": ">=2.11",
    "haakco/laravel-postgres-helper": "@dev",
    "illuminate/support": "~7 || ~8 || ~9 || ~10",
    "torann/geoip": ">=3.0"
  },
  "require-dev": {
    "brainmaestro/composer-git-hooks": ">=2.8",
    "doctrine/dbal": ">=2.13",
    "ergebnis/composer-normalize": ">=2.15",
    "orchestra/testbench": "~5 || ~6 || ~7 || ~8 || ~9 || ~10",
    "phpunit/phpunit": ">=9.5",
    "roave/security-advisories": "dev-latest",
    "symplify/easy-coding-standard": ">=9.4"
  },
  "repositories": {
    "haakco/laravel-postgres-helper": {
      "type": "vcs",
      "url": "git@github.com:haakco/laravel-postgres-helper.git"
    }
  },
  "autoload": {
    "psr-4": {
      "HaakCo\\Ip\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HaakCo\\Ip\\Tests\\": "tests"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true
    }
  },
  "extra": {
    "laravel": {
      "aliases": {
        "Ip": "HaakCo\\Ip\\Facades\\Ip"
      },
      "providers": [
        "HaakCo\\Ip\\IpServiceProvider"
      ]
    }
  },
  "scripts": {
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "vendor/bin/cghooks add --ignore-lock"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "vendor/bin/cghooks update",
      "composer normalize"
    ],
    "cghooks": "vendor/bin/cghooks",
    "phpcs": [
      "@php vendor/bin/ecs check --no-interaction",
      "@php vendor/bin/ecs check --fix --no-interaction"
    ],
    "phpcs-check": [
      "@php vendor/bin/ecs check --no-interaction"
    ],
    "phpcs-no-cache": [
      "@php vendor/bin/ecs check --clear-cache --no-interaction",
      "@php vendor/bin/ecs check --no-interaction",
      "@php vendor/bin/ecs check --fix --no-interaction"
    ]
  }
}
